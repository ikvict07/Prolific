<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<AnchorPane id="gradientBox" fx:id="gradientBox" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.nevertouchgrass.prolific.javafxcontroller.ProjectPanelController">
    <HBox fx:id="projectInfo" alignment="CENTER_LEFT" spacing="20.0" stylesheets="/css/styles.css">
        <HBox fx:id="projectIcon" alignment="CENTER" prefWidth="20.0" spacing="10.0" stylesheets="/css/styles.css" styleClass="projectIcon">
            <HBox.margin>
                <Insets left="10.0" right="10.0" top="10.0" bottom="10.0"/>
            </HBox.margin>
            <Text fx:id="projectIconText" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="IJ"
                  textAlignment="CENTER">
                <font>
                    <Font name="JetBrainsMono NF Regular"/>
                </font>
            </Text>
        </HBox>
        <Label fx:id="projectTitleText" text="test" textFill="WHITE"/>
        <fx:include source="/icons/fxml/runButton.fxml"/>
        <fx:include source="/icons/fxml/configurationButton.fxml" HBox.hgrow="ALWAYS"/>
    </HBox>
</AnchorPane>
